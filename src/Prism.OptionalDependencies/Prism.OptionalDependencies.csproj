<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <Product>Prism.OptionalDependencies</Product>
    <Description>Provides custom prism module catalogs which support optional dependencies</Description>
    <PackageId>Prism.OptionalDependencies</PackageId>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageReleaseNotes>Inital Release
1.0.0.2 Compiled with xml documentation</PackageReleaseNotes>
    <Version>1.0.0.2</Version>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <Company>Vistron GmbH</Company>
    <Authors>Manuel Blattner</Authors>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>D:\Projects\Prism.OptionalDependencies\src\Prism.OptionalDependencies\Prism.OptionalDependencies.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Prism.Core" Version="7.2.0.1422" />
    <PackageReference Include="Prism.Wpf" Version="7.2.0.1422" />
  </ItemGroup>
</Project>
